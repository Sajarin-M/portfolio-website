    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "pretty-quick": "^3.1.3",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "unocss": "^0.53.1",
